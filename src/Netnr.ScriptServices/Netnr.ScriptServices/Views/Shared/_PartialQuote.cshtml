@model string
@{
    var listQuote = Model.Split(',').ToList();
}
@foreach (var item in listQuote)
{
    switch (item)
    {
        case "fa.css":
            <link href="https://code.bdstatic.com/npm/font-awesome@4.7.0/css/font-awesome.min.css" rel="stylesheet">
            break;

        case "jquery.js":
            <script src="https://code.bdstatic.com/npm/jquery@3.4.1/dist/jquery.min.js"></script>
            break;

        case "bootstrap.css":
            <link href="https://code.bdstatic.com/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" rel="stylesheet">
            break;

        case "bootstrap.js":
            <script src="https://code.bdstatic.com/npm/bootstrap@4.4.1/dist/js/bootstrap.bundle.min.js"></script>
            break;

        case "webuploader.js":
            <script src="https://code.bdstatic.com/npm/webuploader@0.1.8/dist/webuploader.html5only.min.js"></script>
            break;

        case "qrcode.js":
            <script src="https://code.bdstatic.com/npm/jquery.qrcode@1.0.3/jquery.qrcode.min.js"></script>
            break;

        case "bmob.js":
            <script src="/js/plus/Bmob-1.7.0.min.js" asp-append-version="true"></script>
            break;

        case "xml2json.js":
            <script src="https://cdn.jsdelivr.net/gh/abdolence/x2js@1.2.0/xml2json.min.js"></script>
            break;

        case "sql-formatter.js":
            <script src="https://code.bdstatic.com/npm/sql-formatter@2.3.3/dist/sql-formatter.min.js"></script>
            break;

        case "monaco-editor":
            <script src="https://code.bdstatic.com/npm/monaco-editor@0.18.1/min/vs/loader.js"></script>
            <script>
                require.config({
                    paths: { vs: "https://code.bdstatic.com/npm/monaco-editor@0.18.1/min/vs" },
                    'vs/nls': { availableLanguages: { '*': 'zh-cn' } }
                });
            </script>
            break;

        case "loading":
            <div id="LoadingMask" style="position: fixed; top: 0; left: 0; bottom: 0; right: 0; background-color: white; z-index: 9999; background-image:url('/images/loading.svg');background-repeat:no-repeat;background-position:48% 45%"></div>
            break;

        case "the":
            <!--
            https://github.com/netnr
            https://gitee.com/netnr
            https://www.netnr.com
            https://zme.ink
            -->
            break;
    }
}