@{
    var mj = Netnr.Extend.ToJObject(Netnr.Core.FileTo.ReadText(Startup.HostingEnvironment.WebRootPath + "/js/data/", "favorite.json"));
    string title = string.Empty;
    string action = ViewContext.RouteData.Values["action"].ToString().ToLower();
    if (action == "index")
    {
        title = "服务";
    }
    else
    {
        foreach (var gi in mj["links"])
        {
            foreach (Newtonsoft.Json.Linq.JToken jt in gi["items"])
            {
                if (jt["url"].ToString().ToLower() == action)
                {
                    title = jt["title"].ToString();
                    goto endfor;
                }
            }
        }
    }
endfor: TempData["Title"] = title;
}
